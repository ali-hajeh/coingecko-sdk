{
  "openapi": "3.0.0",
  "info": {
    "version": "1.0.1",
    "title": "CoinGecko API",
    "license": {
      "name": "MIT"
    }
  },
  "servers": [
    {
      "url": "https://pro-api.coingecko.com/api/v3",
      "description": "Pro API server"
    },
    {
      "url": "https://api.coingecko.com/api/v3",
      "description": "Public API server"
    }
  ],
  "paths": {
    "/coins/list": {
      "get": {
        "summary": "List all coins",
        "operationId": "fetchCoinList",
        "tags": ["coins"],
        "responses": {
          "200": {
            "description": "A list of all coins",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CoinList"
                }
              }
            }
          },
          "default": {
            "description": "Unexpected error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    },
    "/coins/{id}": {
      "get": {
        "summary": "Get information for a specific coin",
        "operationId": "fetchCoinById",
        "tags": ["coins"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "The ID of the coin to retrieve",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Information for the specified coin",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Coin"
                }
              }
            }
          },
          "default": {
            "description": "Unexpected error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    },
    "/coins/markets": {
      "get": {
        "summary": "List all markets",
        "operationId": "fetchCoinMarkets",
        "tags": ["coins"],
        "parameters": [
          {
            "name": "vs_currency",
            "in": "query",
            "required": true,
            "description": "The target currency of market data (usd, eur, jpy, etc.)",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "order",
            "in": "query",
            "description": "Sort results by market cap, volume, etc.",
            "schema": {
              "type": "string",
              "default": "market_cap_desc"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Market data",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Market"
                  }
                }
              }
            }
          },
          "default": {
            "description": "Unexpected error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    },
    "/coins/{id}/tickers": {
      "get": {
        "summary": "Get all tickers for a specific coin",
        "operationId": "fetchCoinTickers",
        "tags": ["coins"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "The ID of the coin to retrieve tickers for",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Tickers for the specified coin",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TickerList"
                }
              }
            }
          }
        }
      }
    },
    "/coins/{id}/history": {
      "get": {
        "summary": "Get historical data for a specific coin",
        "operationId": "fetchCoinHistory",
        "tags": ["coins"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "The ID of the coin to retrieve historical data for",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "date",
            "in": "query",
            "required": true,
            "description": "The date for historical data in YYYY-MM-DD format",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Historical data for the specified coin",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CoinHistory"
                }
              }
            }
          }
        }
      }
    },
    "/coins/{id}/market_chart": {
      "get": {
        "summary": "Get market chart data for a specific coin",
        "operationId": "fetchCoinMarketChart",
        "tags": ["coins"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "The ID of the coin to retrieve market chart data for",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "vs_currency",
            "in": "query",
            "required": true,
            "description": "The target currency of market data",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "days",
            "in": "query",
            "description": "The number of days of market chart data to retrieve",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Market chart data for the specified coin",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/MarketChart"
                }
              }
            }
          }
        }
      }
    },
    "/coins/{id}/market_chart/range": {
      "get": {
        "summary": "Get market chart data within a range for a specific coin",
        "operationId": "fetchCoinMarketChartRange",
        "tags": ["coins"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "The ID of the coin to retrieve market chart data for",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "vs_currency",
            "in": "query",
            "required": true,
            "description": "The target currency of market data",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "from",
            "in": "query",
            "required": true,
            "description": "The start date in UNIX timestamp",
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "to",
            "in": "query",
            "required": true,
            "description": "The end date in UNIX timestamp",
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Market chart data within the specified range for the coin",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/MarketChartRange"
                }
              }
            }
          }
        }
      }
    },
