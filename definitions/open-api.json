            "description": "The start date in UNIX timestamp",
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "to",
            "in": "query",
            "required": true,
            "description": "The end date in UNIX timestamp",
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OHLC data within the specified range for the coin",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/OHLC"
                }
              }
            }
          }
        }
      }
    },
