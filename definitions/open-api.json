    "/simple/price": {
      "get": {
        "summary": "Get the current price of one or more cryptocurrencies",
        "operationId": "fetchSimplePrice",
        "tags": ["simple"],
        "parameters": [
          {
            "name": "ids",
            "in": "query",
            "required": true,
            "description": "A comma-separated list of coin IDs",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "vs_currencies",
            "in": "query",
            "required": true,
            "description": "A comma-separated list of target currencies",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Current price data for the specified coins",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "additionalProperties": {
                    "type": "object",
                    "properties": {
                      "price": {
                        "type": "number"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/networks": {
      "get": {
        "summary": "List all supported networks",
        "operationId": "fetchNetworksList",
        "tags": ["networks"],
        "responses": {
          "200": {
            "description": "A list of all supported networks",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string"
                      },
                      "name": {
                        "type": "string"
                      },
                      "chain": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "ApiKeyAuth": {
        "type": "apiKey",
        "in": "header",
        "name": "x-cg-pro-api-key"
      }
    },
    "schemas": {
      "Coin": {
        "type": "object",
        "required": ["id", "symbol", "name"],
        "properties": {
          "id": {
            "type": "string"
          },
          "symbol": {
            "type": "string"
          },
          "name": {
            "type": "string"
          }
        }
      },
      "CoinList": {
        "type": "array",
        "items": {
          "$ref": "#/components/schemas/Coin"
        }
      },
      "Market": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "symbol": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "image": {
            "type": "string"
          },
          "current_price": {
            "type": "number"
          },
          "market_cap": {
            "type": "number"
          },
          "total_volume": {
            "type": "number"
          }
        }
      },
      "TickerList": {
        "type": "object",
        "properties": {
          "tickers": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "market": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string"
                    },
                    "name": {
                      "type": "string"
                    }
                  }
                },
                "base": {
                  "type": "string"
                },
                "target": {
                  "type": "string"
                },
                "last": {
                  "type": "number"
                },
                "volume": {
                  "type": "number"
                }
              }
            }
          }
        }
      },
      "CoinHistory": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "symbol": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "market_data": {
            "type": "object",
            "properties": {
              "current_price": {
                "type": "object",
                "properties": {
                  "usd": {
                    "type": "number"
                  }
                }
              }
            }
          }
        }
      },
      "MarketChart": {
        "type": "object",
        "properties": {
          "prices": {
            "type": "array",
            "items": {
              "type": "array",
              "items": {
                "type": "number"
              }
            }
          },
          "market_caps": {
            "type": "array",
            "items": {
              "type": "array",
              "items": {
                "type": "number"
              }
            }
          },
          "total_volumes": {
            "type": "array",
            "items": {
              "type": "array",
              "items": {
                "type": "number"
              }
            }
          }
        }
      },
      "MarketChartRange": {
        "type": "object",
        "properties": {
          "prices": {
            "type": "array",
            "items": {
              "type": "array",
              "items": {
                "type": "number"
              }
            }
          },
          "market_caps": {
            "type": "array",
            "items": {
              "type": "array",
              "items": {
                "type": "number"
              }
            }
          },
          "total_volumes": {
            "type": "array",
            "items": {
              "type": "array",
              "items": {
                "type": "number"
              }
            }
          }
        }
      },
      "OHLC": {
        "type": "array",
        "items": {
          "type": "array",
          "items": {
            "type": "number"
          }
        }
      },
      "CirculatingSupplyChart": {
        "type": "object",
        "properties": {
          "circulating_supply": {
            "type": "array",
            "items": {
              "type": "array",
              "items": {
                "type": "number"
              }
            }
          }
        }
      },
      "TotalSupplyChart": {
        "type": "object",
        "properties": {
          "total_supply": {
            "type": "array",
            "items": {
              "type": "array",
              "items": {
                "type": "number"
              }
            }
          }
        }
      },
      "Network": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "chain": {
            "type": "string"
          }
        }
      },
      "Error": {
        "type": "object",
        "required": ["code", "message"],
        "properties": {
          "code": {
            "type": "integer",
            "format": "int32"
          },
          "message": {
            "type": "string"
          }
        }
      }
    }
  },
  "security": [
    {
      "ApiKeyAuth": []
    }
  ]
}
